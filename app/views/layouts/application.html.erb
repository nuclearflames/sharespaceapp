<!DOCTYPE html>
<html>
  <head>
    <title>Sharespace!</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container-fluid text-center">
      <div class="row">
        <div class="col-md-10 col-md-offset-1 main">
          <div class="row">
            <div class="col-md-12">
              <h1>Welcome to share space!</h1>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <%= yield %>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <h1>Store your files on our cloud!</h1>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
